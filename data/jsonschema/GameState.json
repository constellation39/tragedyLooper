{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/GameState",
    "definitions": {
        "GameState": {
            "properties": {
                "game_id": {
                    "type": "string",
                    "description": "游戏唯一ID"
                },
                "script": {
                    "$ref": "#/definitions/v1.Script",
                    "additionalProperties": true,
                    "description": "当前游戏的剧本配置"
                },
                "characters": {
                    "additionalProperties": {
                        "$ref": "#/definitions/v1.Character",
                        "additionalProperties": true
                    },
                    "type": "object",
                    "description": "所有角色当前状态的映射，键为 character_id"
                },
                "players": {
                    "additionalProperties": {
                        "$ref": "#/definitions/v1.Player",
                        "additionalProperties": true
                    },
                    "type": "object",
                    "description": "所有玩家的映射，键为 player_id"
                },
                "current_day": {
                    "type": "integer",
                    "description": "当前天数"
                },
                "current_loop": {
                    "type": "integer",
                    "description": "当前循环数"
                },
                "current_phase": {
                    "enum": [
                        "GAME_PHASE_UNSPECIFIED",
                        0,
                        "SETUP",
                        1,
                        "MASTERMIND_SETUP",
                        2,
                        "CARD_PLAY",
                        3,
                        "CARD_REVEAL",
                        4,
                        "CARD_RESOLVE",
                        5,
                        "ABILITIES",
                        6,
                        "INCIDENTS",
                        7,
                        "DAY_END",
                        8,
                        "LOOP_END",
                        9,
                        "GAME_OVER",
                        10,
                        "PROTAGONIST_GUESS",
                        11,
                        "FIRST_GUESS",
                        12
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Game Phase",
                    "description": "游戏阶段枚举"
                },
                "active_tragedies": {
                    "additionalProperties": {
                        "type": "boolean"
                    },
                    "type": "object",
                    "description": "当前循环中已激活（满足条件）的悲剧，键为 TragedyType"
                },
                "prevented_tragedies": {
                    "additionalProperties": {
                        "type": "boolean"
                    },
                    "type": "object",
                    "description": "当前循环中已被阻止的悲剧，键为 TragedyType"
                },
                "played_cards_this_day": {
                    "additionalProperties": {
                        "$ref": "#/definitions/v1.Card",
                        "additionalProperties": true
                    },
                    "type": "object",
                    "description": "Cards played today, keyed by player_id."
                },
                "played_cards_this_loop": {
                    "additionalProperties": {
                        "type": "boolean"
                    },
                    "type": "object",
                    "description": "IDs of cards played in this loop."
                },
                "last_update_time": {
                    "type": "string",
                    "description": "最后更新时间戳 (Unix timestamp)"
                },
                "day_events": {
                    "items": {
                        "$ref": "#/definitions/v1.GameEvent"
                    },
                    "type": "array",
                    "description": "本日发生的事件日志"
                },
                "loop_events": {
                    "items": {
                        "$ref": "#/definitions/v1.GameEvent"
                    },
                    "type": "array",
                    "description": "本循环发生的事件日志"
                },
                "character_paranoia_limits": {
                    "additionalProperties": {
                        "type": "integer"
                    },
                    "type": "object",
                    "description": "每个角色的妄想上限映射（新增）"
                },
                "character_goodwill_limits": {
                    "additionalProperties": {
                        "type": "integer"
                    },
                    "type": "object",
                    "description": "每个角色的好感度上限映射（新增）"
                },
                "character_intrigue_limits": {
                    "additionalProperties": {
                        "type": "integer"
                    },
                    "type": "object",
                    "description": "每个角色的阴谋上限映射（新增）"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Game State",
            "description": "游戏当前状态"
        },
        "v1.Ability": {
            "properties": {
                "id": {
                    "type": "integer",
                    "description": "The ID of the ability, linking to an AbilityConfig"
                },
                "used_this_loop": {
                    "type": "boolean",
                    "description": "Runtime state: whether this ability has been used in the current loop"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Ability",
            "description": "Ability represents a runtime instance of an ability."
        },
        "v1.Card": {
            "properties": {
                "id": {
                    "type": "integer",
                    "description": "The ID of the card, linking to a CardConfig"
                },
                "used_this_loop": {
                    "type": "boolean",
                    "description": "Runtime state: whether this card has been used in the current loop"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Card",
            "description": "Card represents a runtime instance of a card."
        },
        "v1.Character": {
            "properties": {
                "id": {
                    "type": "integer",
                    "description": "The ID of the character, linking to CharacterData"
                },
                "current_location": {
                    "enum": [
                        "LOCATION_TYPE_UNSPECIFIED",
                        0,
                        "HOSPITAL",
                        1,
                        "SHRINE",
                        2,
                        "CITY",
                        3,
                        "SCHOOL",
                        4
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Location Type",
                    "description": "地点类型枚举"
                },
                "paranoia": {
                    "type": "integer",
                    "description": "Current paranoia level"
                },
                "goodwill": {
                    "type": "integer",
                    "description": "Current goodwill level"
                },
                "intrigue": {
                    "type": "integer",
                    "description": "Current intrigue level"
                },
                "hidden_role": {
                    "enum": [
                        "ROLE_TYPE_UNSPECIFIED",
                        0,
                        "INNOCENT",
                        1,
                        "KILLER",
                        2,
                        "ACCOMPLICE",
                        3,
                        "PERSON_OF_INTEREST",
                        4,
                        "KEY_PERSON",
                        5,
                        "FRIEND",
                        6,
                        "LOVER",
                        7,
                        "CONSPIRACY_THEORIST",
                        8,
                        "CULTIST",
                        9
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Role Type",
                    "description": "角色身份类型枚举"
                },
                "abilities": {
                    "items": {
                        "$ref": "#/definitions/v1.Ability"
                    },
                    "type": "array",
                    "description": "List of abilities the character possesses"
                },
                "is_alive": {
                    "type": "boolean",
                    "description": "Whether the character is alive"
                },
                "in_panic_mode": {
                    "type": "boolean",
                    "description": "Whether the character is in panic mode"
                },
                "paranoia_limit": {
                    "type": "integer",
                    "description": "Paranoia limit"
                },
                "goodwill_limit": {
                    "type": "integer",
                    "description": "Goodwill limit"
                },
                "intrigue_limit": {
                    "type": "integer",
                    "description": "Intrigue limit"
                },
                "rules": {
                    "items": {
                        "$ref": "#/definitions/v1.CharacterRule"
                    },
                    "type": "array",
                    "description": "List of special rules affecting the character"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Character",
            "description": "Character represents the dynamic, runtime state of a character in a game."
        },
        "v1.CharacterConfig": {
            "properties": {
                "id": {
                    "type": "integer",
                    "description": "Character ID"
                },
                "hidden_role": {
                    "enum": [
                        "ROLE_TYPE_UNSPECIFIED",
                        0,
                        "INNOCENT",
                        1,
                        "KILLER",
                        2,
                        "ACCOMPLICE",
                        3,
                        "PERSON_OF_INTEREST",
                        4,
                        "KEY_PERSON",
                        5,
                        "FRIEND",
                        6,
                        "LOVER",
                        7,
                        "CONSPIRACY_THEORIST",
                        8,
                        "CULTIST",
                        9
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Role Type",
                    "description": "角色身份类型枚举"
                },
                "initial_location": {
                    "enum": [
                        "LOCATION_TYPE_UNSPECIFIED",
                        0,
                        "HOSPITAL",
                        1,
                        "SHRINE",
                        2,
                        "CITY",
                        3,
                        "SCHOOL",
                        4
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Location Type",
                    "description": "地点类型枚举"
                },
                "initial_paranoia": {
                    "type": "integer",
                    "description": "Initial paranoia"
                },
                "initial_goodwill": {
                    "type": "integer",
                    "description": "Initial goodwill"
                },
                "initial_intrigue": {
                    "type": "integer",
                    "description": "Initial intrigue"
                },
                "initial_ability_ids": {
                    "items": {
                        "type": "integer"
                    },
                    "type": "array",
                    "description": "List of initial ability IDs"
                },
                "is_first_step_role": {
                    "type": "boolean",
                    "description": "Whether this is a First Step role for this script"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Character Config",
            "description": "CharacterConfig defines a character's initial setup for a specific script."
        },
        "v1.CharacterRule": {
            "properties": {
                "trigger": {
                    "enum": [
                        "TRIGGER_TYPE_UNSPECIFIED",
                        0,
                        "ON_GAME_SETUP",
                        1,
                        "ON_LOOP_START",
                        2,
                        "ON_DAY_START",
                        3,
                        "ON_CARD_PLAY_PHASE",
                        4,
                        "ON_CARD_REVEAL_PHASE",
                        5,
                        "ON_CARD_RESOLVE_PHASE",
                        6,
                        "ON_ABILITY_PHASE",
                        7,
                        "ON_INCIDENT_PHASE",
                        8,
                        "ON_GAME_EVENT",
                        9,
                        "PASSIVE",
                        10,
                        "ON_LOOP_LOSS",
                        11,
                        "ON_LOOP_WIN",
                        12,
                        "ON_GAME_END",
                        13
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Trigger Type",
                    "description": "触发器类型枚举（能力或规则何时触发）"
                },
                "description": {
                    "type": "string",
                    "description": "Description of the rule"
                },
                "turf_selection_effect": {
                    "$ref": "#/definitions/v1.TurfSelectionEffect",
                    "additionalProperties": true,
                    "description": "Effect for selecting a starting location"
                },
                "delayed_entry_effect": {
                    "$ref": "#/definitions/v1.DelayedEntryEffect",
                    "additionalProperties": true,
                    "description": "Effect for delayed entry into the game"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Character Rule",
            "description": "CharacterRule defines a special rule that applies to a character."
        },
        "v1.Condition": {
            "properties": {
                "stat_condition": {
                    "$ref": "#/definitions/v1.StatCondition",
                    "additionalProperties": true,
                    "description": "属性条件（例如，妄想值达到某阈值）"
                },
                "location_condition": {
                    "$ref": "#/definitions/v1.LocationCondition",
                    "additionalProperties": true,
                    "description": "地点条件（例如，角色在特定地点）"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Condition",
            "description": "条件的通用消息，使用 oneof 包含各种具体条件"
        },
        "v1.DelayedEntryEffect": {
            "properties": {
                "day_of_entry": {
                    "type": "integer",
                    "description": "The day the character enters"
                },
                "entry_location": {
                    "enum": [
                        "LOCATION_TYPE_UNSPECIFIED",
                        0,
                        "HOSPITAL",
                        1,
                        "SHRINE",
                        2,
                        "CITY",
                        3,
                        "SCHOOL",
                        4
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Location Type",
                    "description": "地点类型枚举"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Delayed Entry Effect",
            "description": "DelayedEntryEffect causes a character to enter the game on a specific day."
        },
        "v1.GameEndCondition": {
            "properties": {
                "type": {
                    "enum": [
                        "CONDITION_TYPE_UNSPECIFIED",
                        0,
                        "ALL_TRAGEDIES_PREVENTED",
                        1,
                        "SPECIFIC_TRAGEDY_TRIGGERED",
                        2,
                        "MAX_LOOPS_REACHED",
                        3,
                        "PROTAGONIST_GUESSED_CORRECTLY",
                        4,
                        "PROTAGONIST_GUESSED_INCORRECTLY",
                        5
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Condition Type",
                    "description": "条件类型"
                },
                "incident_type": {
                    "enum": [
                        "INCIDENT_TYPE_UNSPECIFIED",
                        0,
                        "MURDER",
                        1,
                        "SUICIDE",
                        2,
                        "HOSPITAL_CONSPIRACY",
                        3,
                        "MURDER_MYSTERY",
                        4
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Incident Type",
                    "description": "悲剧类型枚举"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Game End Condition",
            "description": "游戏结束条件"
        },
        "v1.GameEvent": {
            "properties": {
                "type": {
                    "enum": [
                        "GAME_EVENT_TYPE_UNSPECIFIED",
                        0,
                        "CHARACTER_MOVED",
                        1,
                        "PARANOIA_ADJUSTED",
                        2,
                        "GOODWILL_ADJUSTED",
                        3,
                        "INTRIGUE_ADJUSTED",
                        4,
                        "INCIDENT_TRIGGERED",
                        5,
                        "INCIDENT_PREVENTED",
                        6,
                        "ROLE_REVEALED",
                        7,
                        "ABILITY_GRANTED",
                        8,
                        "CHOICE_REQUIRED",
                        9,
                        "LOOP_WIN",
                        10,
                        "LOOP_LOSS",
                        11,
                        "FIRST_GUESS_MADE",
                        12,
                        "FINAL_GUESS_MADE",
                        13,
                        "GOODWILL_REFUSAL_TEST",
                        14,
                        "LOOP_OVER",
                        15,
                        "DAY_ADVANCED",
                        16,
                        "TRAGEDY_TRIGGERED",
                        17,
                        "LOOP_RESET",
                        18
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Game Event Type",
                    "description": "游戏事件类型枚举"
                },
                "payload": {
                    "properties": {
                        "type_url": {
                            "type": "string",
                            "description": "A URL/resource name that uniquely identifies the type of the serialized protocol buffer message. This string must contain at least one \"/\" character. The last segment of the URL's path must represent the fully qualified name of the type (as in `path/google.protobuf.Duration`). The name should be in a canonical form (e.g., leading \".\" is not accepted). In practice, teams usually precompile into the binary all types that they expect it to use in the context of Any. However, for URLs which use the scheme `http`, `https`, or no scheme, one can optionally set up a type server that maps type URLs to message definitions as follows: * If no scheme is provided, `https` is assumed. * An HTTP GET on the URL must yield a [google.protobuf.Type][]   value in binary format, or produce an error. * Applications are allowed to cache lookup results based on the   URL, or have them precompiled into a binary to avoid any   lookup. Therefore, binary compatibility needs to be preserved   on changes to types. (Use versioned type names to manage   breaking changes.) Note: this functionality is not currently available in the official protobuf release, and it is not used for type URLs beginning with type.googleapis.com. As of May 2023, there are no widely used type server implementations and no plans to implement one. Schemes other than `http`, `https` (or the empty scheme) might be used with implementation specific semantics."
                        },
                        "value": {
                            "type": "string",
                            "description": "Must be a valid serialized protocol buffer of the above specified type.",
                            "format": "binary",
                            "binaryEncoding": "base64"
                        }
                    },
                    "additionalProperties": true,
                    "type": "object"
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Game Event",
            "description": "GameEvent 表示游戏中发生的事件。"
        },
        "v1.IncidentConfig": {
            "properties": {
                "incident_type": {
                    "enum": [
                        "INCIDENT_TYPE_UNSPECIFIED",
                        0,
                        "MURDER",
                        1,
                        "SUICIDE",
                        2,
                        "HOSPITAL_CONSPIRACY",
                        3,
                        "MURDER_MYSTERY",
                        4
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Incident Type",
