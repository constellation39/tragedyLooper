                            1,
                            "STAT_ADJUSTED",
                            2,
                            "INCIDENT_TRIGGERED",
                            3,
                            "ROLE_REVEALED",
                            4,
                            "CHARACTER_DIED",
                            5,
                            "ABILITY_USED",
                            6,
                            "CARD_PLAYED",
                            7,
                            "DAY_ADVANCED",
                            8,
                            "LOOP_RESET",
