                            1,
                            "KILLER",
                            2,
                            "ACCOMPLICE",
                            3,
                            "PERSON_OF_INTEREST",
                            4,
                            "KEY_PERSON",
                            5,
                            "FRIEND",
                            6,
                            "LOVER",
                            7,
                            "CONSPIRACY_THEORIST",
                            8,
                            "CULTIST",
                            9
