                },
                "target_type": {
                    "type": "string"
                },
                "effect": {
                    "$ref": "#/definitions/model.Effect",
                    "additionalProperties": true
                },
                "once_per_loop": {
                    "type": "boolean"
                },
                "used_this_loop": {
                    "type": "boolean"
                },
                "target_character_id": {
                    "type": "string"
                },
                "target_location": {
                    "enum": [
                        "LOCATION_TYPE_UNSPECIFIED",
                        0,
                        "LOCATION_TYPE_HOSPITAL",
                        1,
                        "LOCATION_TYPE_SHRINE",
                        2,
                        "LOCATION_TYPE_CITY",
                        3,
                        "LOCATION_TYPE_SCHOOL",
                        4
