{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/PlayerView",
    "definitions": {
        "PlayerView": {
            "properties": {
                "game_id": {
                    "type": "string"
                },
                "script_id": {
                    "type": "string"
                },
                "characters": {
                    "additionalProperties": {
                        "$ref": "#/definitions/model.Character",
                        "additionalProperties": true
                    },
                    "type": "object"
                },
                "players": {
                    "additionalProperties": {
                        "$ref": "#/definitions/model.Player",
                        "additionalProperties": true
                    },
                    "type": "object"
                },
                "current_day": {
                    "type": "integer"
                },
                "current_loop": {
                    "type": "integer"
                },
                "current_phase": {
                    "enum": [
                        "GAME_PHASE_UNSPECIFIED",
                        0,
                        "GAME_PHASE_MORNING",
                        1,
                        "GAME_PHASE_CARD_PLAY",
                        2,
                        "GAME_PHASE_CARD_REVEAL",
                        3,
                        "GAME_PHASE_CARD_RESOLVE",
                        4,
                        "GAME_PHASE_ABILITIES",
                        5,
                        "GAME_PHASE_INCIDENTS",
                        6,
                        "GAME_PHASE_DAY_END",
                        7,
                        "GAME_PHASE_LOOP_END",
                        8,
                        "GAME_PHASE_GAME_OVER",
                        9,
                        "GAME_PHASE_PROTAGONIST_GUESS",
                        10
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Game Phase",
                    "description": "GamePhase 定义游戏日期的当前阶段。"
                },
                "active_tragedies": {
                    "additionalProperties": {
                        "type": "boolean"
                    },
                    "type": "object"
                },
                "prevented_tragedies": {
                    "additionalProperties": {
                        "type": "boolean"
                    },
                    "type": "object"
                },
                "your_hand": {
                    "items": {
                        "$ref": "#/definitions/model.Card"
                    },
                    "type": "array"
                },
                "your_deductions": {
                    "additionalProperties": {
                        "oneOf": [
                            {
                                "type": "array"
                            },
                            {
                                "type": "boolean"
                            },
                            {
                                "type": "number"
                            },
                            {
                                "type": "object"
                            },
                            {
                                "type": "string"
                            }
                        ],
                        "title": "Value",
                        "description": "`Value` represents a dynamically typed value which can be either null, a number, a string, a boolean, a recursive struct value, or a list of values. A producer of value is expected to set one of these variants. Absence of any variant indicates an error. The JSON representation for `Value` is JSON value."
                    },
                    "type": "object"
                },
                "public_events": {
                    "items": {
                        "$ref": "#/definitions/model.GameEvent"
                    },
                    "type": "array"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Player View",
            "description": "PlayerView 表示特定玩家的游戏状态过滤视图。"
        },
        "model.Ability": {
            "properties": {
                "id": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "trigger_type": {
                    "enum": [
                        "ABILITY_TRIGGER_TYPE_UNSPECIFIED",
                        0,
                        "ABILITY_TRIGGER_TYPE_DAY_START",
                        1,
                        "ABILITY_TRIGGER_TYPE_MASTERMIND_PHASE",
                        2,
                        "ABILITY_TRIGGER_TYPE_GOODWILL_PHASE",
                        3,
                        "ABILITY_TRIGGER_TYPE_PASSIVE",
                        4
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Ability Trigger Type",
                    "description": "AbilityTriggerType 定义能力何时可以被触发。"
                },
                "effect": {
                    "$ref": "#/definitions/model.Effect",
                    "additionalProperties": true
                },
                "once_per_loop": {
                    "type": "boolean"
                },
                "refusal_role": {
                    "enum": [
                        "PLAYER_ROLE_UNSPECIFIED",
                        0,
                        "PLAYER_ROLE_MASTERMIND",
                        1,
                        "PLAYER_ROLE_PROTAGONIST",
                        2
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Player Role",
                    "description": "PlayerRole 定义连接玩家的角色（人类或 LLM）。"
                },
                "used_this_loop": {
                    "type": "boolean"
                },
                "target_location": {
                    "enum": [
                        "LOCATION_TYPE_UNSPECIFIED",
                        0,
                        "LOCATION_TYPE_HOSPITAL",
                        1,
                        "LOCATION_TYPE_SHRINE",
                        2,
                        "LOCATION_TYPE_CITY",
                        3,
                        "LOCATION_TYPE_SCHOOL",
                        4
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Location Type",
                    "description": "LocationType 定义游戏中的可能地点。"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Ability",
            "description": "Ability 定义角色的特殊技能。"
        },
        "model.AdjustGoodwillEffect": {
            "properties": {
                "amount": {
                    "type": "integer"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Adjust Goodwill Effect",
            "description": "AdjustGoodwillEffect 调整角色的好感度。"
        },
        "model.AdjustIntrigueEffect": {
            "properties": {
                "amount": {
                    "type": "integer"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Adjust Intrigue Effect",
            "description": "AdjustIntrigueEffect 调整角色的阴谋值。"
        },
        "model.AdjustParanoiaEffect": {
