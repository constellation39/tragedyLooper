            "additionalProperties": true,
            "type": "object",
            "title": "Card",
            "description": "Card 表示一张行动卡。"
        },
        "model.Character": {
            "properties": {
                "id": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "traits": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
